{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "aql",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "query"
        },
        {
          "type": "SYMBOL",
          "name": "return_operation"
        }
      ]
    },
    "query": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "for_operation"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "filter_operation"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "return_operation"
        }
      ]
    },
    "for_operation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_for"
        },
        {
          "type": "FIELD",
          "name": "variable",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "kw_in"
        },
        {
          "type": "FIELD",
          "name": "collection",
          "content": {
            "type": "SYMBOL",
            "name": "collection_selector"
          }
        }
      ]
    },
    "filter_operation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_filter"
        },
        {
          "type": "SYMBOL",
          "name": "comparison_expression"
        }
      ]
    },
    "return_operation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_return"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "member_expression"
        }
      ]
    },
    "_expression_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "SYMBOL",
          "name": "collection_bind_var"
        }
      ]
    },
    "member_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression_atom"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "object",
              "content": {
                "type": "SYMBOL",
                "name": "member_expression"
              }
            },
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "FIELD",
              "name": "property",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "object",
              "content": {
                "type": "SYMBOL",
                "name": "member_expression"
              }
            },
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "FIELD",
              "name": "property",
              "content": {
                "type": "SYMBOL",
                "name": "expression"
              }
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        }
      ]
    },
    "comparison_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "comparison_operator"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "comparison_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": ">="
        }
      ]
    },
    "collection_selector": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "collection_name"
        },
        {
          "type": "SYMBOL",
          "name": "collection_bind_var"
        }
      ]
    },
    "collection_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "collection_bind_var": {
      "type": "PATTERN",
      "value": "@@[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        }
      ]
    },
    "value": {
      "type": "SYMBOL",
      "name": "literal"
    },
    "object": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "object_pair"
                        },
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "SYMBOL",
                            "name": "identifier"
                          },
                          "named": true,
                          "value": "shorthand_property_identifier"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "SYMBOL",
                                "name": "object_pair"
                              },
                              {
                                "type": "ALIAS",
                                "content": {
                                  "type": "SYMBOL",
                                  "name": "identifier"
                                },
                                "named": true,
                                "value": "shorthand_property_identifier"
                              }
                            ]
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "object_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                "named": true,
                "value": "property_identifier"
              },
              {
                "type": "SYMBOL",
                "name": "string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        }
      ]
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+(\\.\\d+)?([eE][+-]?\\d+)?"
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "unescaped_double_string_fragment"
                    },
                    "named": true,
                    "value": "string_fragment"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "escape_sequence"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "unescaped_single_string_fragment"
                    },
                    "named": true,
                    "value": "string_fragment"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "escape_sequence"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "unescaped_double_string_fragment": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\"\\\\\\r\\n]+"
        }
      }
    },
    "unescaped_single_string_fragment": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^'\\\\\\r\\n]+"
        }
      }
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^xu0-7]"
              },
              {
                "type": "PATTERN",
                "value": "[0-7]{1,3}"
              },
              {
                "type": "PATTERN",
                "value": "x[0-9a-fA-F]{2}"
              },
              {
                "type": "PATTERN",
                "value": "u[0-9a-fA-F]{4}"
              },
              {
                "type": "PATTERN",
                "value": "u\\{[0-9a-fA-F]+\\}"
              },
              {
                "type": "PATTERN",
                "value": "[\\r?][\\n\\u2028\\u2029]"
              }
            ]
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "kw_filter": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[fF][iI][lL][tT][eE][rR]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_return": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[rR][eE][tT][uU][rR][nN]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_in": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[iI][nN]"
        }
      },
      "named": true,
      "value": "keyword_operator"
    },
    "kw_for": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[fF][oO][rR]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_let": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[lL][eE][tT]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_collect": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[cC][oO][lL][lL][eE][cC][tT]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_into": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[iI][nN][tT][oO]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_insert": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[iI][nN][sS][eE][rR][tT]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_update": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[uU][pP][dD][aA][tT][eE]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_upsert": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[uU][pP][sS][eE][rR][tT]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "kw_replace": {
      "type": "ALIAS",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "[rR][eE][pP][lL][aA][cC][eE]"
        }
      },
      "named": true,
      "value": "keyword_operation"
    },
    "boolean": {
      "type": "ALIAS",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_kw_true"
          },
          {
            "type": "SYMBOL",
            "name": "_kw_false"
          }
        ]
      },
      "named": true,
      "value": "boolean_literal"
    },
    "null": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_kw_null"
      },
      "named": true,
      "value": "null_literal"
    },
    "_kw_true": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[tT][rR][uU][eE]"
      }
    },
    "_kw_false": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[fF][aA][lL][sS][eE]"
      }
    },
    "_kw_null": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[nN][uU][lL][lL]"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}