{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "aql",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "_definition"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "_definition": {
      "type": "SYMBOL",
      "name": "statement"
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "iter_collection_expression"
        },
        {
          "type": "SYMBOL",
          "name": "return_statement"
        }
      ]
    },
    "collection_selector": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "collection_name"
        },
        {
          "type": "SYMBOL",
          "name": "collection_bind_var"
        }
      ]
    },
    "iter_collection_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "for"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "in"
        },
        {
          "type": "SYMBOL",
          "name": "collection_selector"
        }
      ]
    },
    "collection_bind_var": {
      "type": "PATTERN",
      "value": "@@[a-z_]+"
    },
    "collection_name": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "return_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "return"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}